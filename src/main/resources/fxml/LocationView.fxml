<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<GridPane fx:id="view" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
   </rowConstraints>
   <columnConstraints>
      <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
   </columnConstraints>
   <children>
      <Label text="LATITUDE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="2">
         <children>
            <Label text="IP LOCATION">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Text fx:id="ipLocationText" strokeType="OUTSIDE" strokeWidth="0.0" text=" ">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </Text>
            <ImageView fx:id="flagImageView" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </ImageView>
         </children>
      </HBox>
      <Label text="CITY, STATE" GridPane.rowIndex="3">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label text="LANGUAGE" GridPane.rowIndex="5">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label text="CALLING CODE" GridPane.rowIndex="7">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label text="LONGITUDE" GridPane.columnIndex="1">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label text="ZIP CODE" GridPane.columnIndex="1" GridPane.rowIndex="3">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label text="TIME ZONE" GridPane.columnIndex="1" GridPane.rowIndex="5">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label text="CURRENCY" GridPane.columnIndex="1" GridPane.rowIndex="7">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Text fx:id="latitudeText" strokeType="OUTSIDE" strokeWidth="0.0" text=" " GridPane.rowIndex="1" />
      <Text fx:id="cityStateText" strokeType="OUTSIDE" strokeWidth="0.0" text=" " GridPane.rowIndex="4" />
      <Text fx:id="languageText" strokeType="OUTSIDE" strokeWidth="0.0" text=" " GridPane.rowIndex="6" />
      <Text fx:id="callingCodeText" strokeType="OUTSIDE" strokeWidth="0.0" text=" " GridPane.rowIndex="8" />
      <Text fx:id="longitudeText" strokeType="OUTSIDE" strokeWidth="0.0" text=" " GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Text fx:id="zipCodeText" strokeType="OUTSIDE" strokeWidth="0.0" text=" " GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <Text fx:id="timeZoneText" strokeType="OUTSIDE" strokeWidth="0.0" text=" " GridPane.columnIndex="1" GridPane.rowIndex="6" />
      <Text fx:id="currencyText" strokeType="OUTSIDE" strokeWidth="0.0" text=" " GridPane.columnIndex="1" GridPane.rowIndex="8" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</GridPane>
